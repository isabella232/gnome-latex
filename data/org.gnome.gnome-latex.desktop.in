[Desktop Entry]
Name=GNOME LaTeX
_GenericName=LaTeX Editor
_Comment=Edit LaTeX documents
_Keywords=text;tex;latex;editor;documents;
Exec=gnome-latex %U
Icon=org.gnome.gnome-latex
Terminal=false
Type=Application
StartupNotify=true
Categories=GNOME;GTK;Office;WordProcessor;Publishing;
MimeType=text/x-tex;
DBusActivatable=true
Actions=new-window;new-document;

[Desktop Action new-window]
_Name=Open a New Window
Exec=gnome-latex --new-window

[Desktop Action new-document]
_Name=Open a New Document
Exec=gnome-latex --new-document
